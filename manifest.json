{
    "name": "pr-mod",
    "version": "1.0",
    "description": "PR review made easier",
    "manifest_version": 2,
    "content_scripts":[
        {
            "js": ["jquery.js", "pr-mod.js"],
            "matches": ["*://*.github.com/*/pull/*"]
        }
    ],
    "permissions":[
        "activeTab",
        "cookies",
        "http://*/*",
        "https://*/*"
    ],
    "browser_action":{
        "default_popup": "popup.html"
    },
    "content_security_policy":"script-src 'self' https://apis.google.com; object-src 'self'"
}